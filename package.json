{
  "private": false,
  "name": "ii-admin-charts",
  "version": "0.1.6",
  "author": "ShiMu",
  "license": "MIT",
  "keywords": ["bizcharts","g2"],
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "prepublishOnly": "npm run build"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
  },
  "devDependencies": {
    "@umijs/test": "^3.0.5",
    "bizcharts": "^4.0.14",
    "dumi": "^1.0.10",
    "father-build": "^1.17.2",
    "gh-pages": "^3.1.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "yorkie": "^2.0.0"
  },
  "peerDependencies": {
    "bizcharts": ">= 4.0.0",
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0"
  }
}
